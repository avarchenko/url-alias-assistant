{
  "manifest_version": 2,
  "name": "URL alias assistant",
  "version": "1.0.3",
  "description": "To use, type 'g' plus a search term into the url bar. You can customize the existing and add own rules in Settings of the plug-in",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "g"
  },
  "options_ui": {
    "chrome_style": false,
    "page": "options.html"
  },
  "icons": {
    "48": "logo48.png",
    "96": "logo96.png"
  },
  "homepage_url": "https://github.com/avarchenko/url-alias-assistant",
  "permissions": [
    "storage"
  ]
}